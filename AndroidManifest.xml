<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.todolists">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Shopping"
            android:label="Shopping List"/>
        <activity android:name=".Housework"
            android:label="Housework List"/>
        <activity android:name=".Gardening"
            android:label="Gardening List"/>
        <activity android:name=".Cooking"
            android:label="Cooking List"/>
        <activity android:name=".Workout"
            android:label="Workout List"></activity>
    </application>

</manifest>
